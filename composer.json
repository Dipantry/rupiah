{
  "name": "dipantry/rupiah",
  "description": "All-in-one Indonesian Rupiah library",
  "keywords": ["rupiah", "indonesia", "bank", "kurs", "money", "currency"],
  "minimum-stability": "dev",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "role": "Developer",
      "name": "Auriga Aristo",
      "email": "aurigaaristo@gmail.com",
      "homepage": "https://aurigaaristo.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^9.0.0",
    "ext-fileinfo": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.0",
    "php-coveralls/php-coveralls": "^2.1",
    "orchestra/testbench": "^7.0",
    "ext-openssl": "*"
  },
  "autoload": {
    "psr-4": {
      "Dipantry\\Rupiah\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dipantry\\Rupiah\\Tests\\": "tests/"
    }
  },
  "prefer-stable": true
}